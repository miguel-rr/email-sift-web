{
  "name": "email-sift-web",
  "display-name": "Email Sift Web",
  "description": "Miguel Rodr√≠guez&#x27;s Email Sift Web",
  "author": "",
  "version": "1.0.0",
  "icon": "assets/blueprint.svg",
  "interfaces": {
    "summary": {
      "controller": "dist/js/controller.umd-es2015.min.js"
    }
  },
  "dag": {
    "inputs":{
      "emails": {
        "gmailEmails":{
          "filter":{
            "conditions":[{
              "date": "between now and 1 week before now"
            }],
            "operator": "AND"
          },
          "wants": [
            "archive",
            "headers",
            "extensions",
            "textBody",
            "strippedHtmlBody"
          ]
        }
      }
    },
    "nodes":[{
      "#": "Parse",
      "implementation": {
        "javascript": "server/parse.js"
      },
      "input": {
          "bucket": "gmailEmails"
      },
      "outputs": {
        "messages-st": {},
        "messages": {}
      }
    },
    {
      "#": "Count",
      "implementation": {
        "javascript": "server/count.js"
      },
      "input": {
        "bucket": "messages-st",
        "select": "*/*"
      },
      "outputs": {
        "count": {}
      }
    }],
    "stores":{
      "messages-st" : {
        "key$schema":"string/string"
      }
    },
    "outputs":{
      "exports":{
        "count" : {
          "key$schema":"string"
        },
        "messages": {
          "key$schema": "string/string"
        }
      }
    }
  }
}
